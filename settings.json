{
  "launchTerminalOnOpen": true,
  "fus.sendAllowed": false,
  "editor.alwaysShowHorizontalScrollbar": true,
  "editor.showStickyLines": true,
  "editor.renderWhitespace": "All",
  "editor.caretShape": "Vertical",
  "editor.softWrapEnabled": true,
  "showEmptyStateOnLastClosedWindow": false,
  "restoreWorkspacesOnStartup": false,
  "editor.fontFamily": "Operator Mono",
  "editor.guides": [60, 72, 80, 85, 120],
  "autoSave": "Off",
  "editor.fontSize": 14.0,
  "editor.lineSpacing": 1.4,
  "editor.honorCamelHumps": true,
  "aiCompletion.enabled": false,
  "completion.inline.highlighting.enabled": false,
  "editor.caretSmoothAnimation": false,
  "warn.before.quitting": true,
  "zoomLevel": 1.1,
  "editor.caretBlinkingEnabled": true,
  "theme": "dark"
}
